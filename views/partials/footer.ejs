<div id="getcall" uk-modal="">
	<div class="uk-modal-dialog uk-modal-body">
		<h2 class="uk-modal-title">
			<%= lang.header.form.contacts%>
		</h2>
		<button class="uk-modal-close-default" type="button" uk-close></button>
		<form id="getcall-form" method="POST" action="/contact-us">
			<input class="uk-input field" type="text" name="name" placeholder="<%= lang.header.form.name%>" autocomplete="off"/>
			<input class="uk-input field" type="phone" name="phone" pattern="^((\+7|7|8)+([0-9]){10})$" placeholder="<%= lang.header.form.phone%>" autocomplete="off"/>
			<button class="button" type="submit"><%= lang.header.form.call%></button>
		</form>
	</div>
</div>
<div id="getorder" uk-modal="">
	<div class="uk-modal-dialog uk-modal-body">
		<button class="uk-modal-close-default" type="button" uk-close></button>
		<h2><%= lang.order_transfer%></h2>
		<form method="POST" encrypt="multipart/form-data" id="modal-form" action="/order-transfer">
			<div class="w50">
				<input class="uk-input field" autocomplete="off" id="name" name="name" type="text" placeholder="<%= lang.order.form.name%>" required />
				<input class="uk-input field" autocomplete="off" id="phone" name="phone" type="text" placeholder="<%= lang.order.form.phone%>" required />
			</div>
			<input class="uk-input field" autocomplete="off" id="email" type="email" name="email" placeholder="<%= lang.order.form.email%>" required />
			<div id="headDropzone" class="dropzone js-upload uk-placeholder uk-text-center needsclick dz-clickable">
				<div class="dz-default dz-message">
					<span class="uk-text-middle"><%= lang.order.form.out_file%></span>
					<div id="clickHere" uk-form-custom="">
						<span class="uk-link"><%= lang.order.form.browse%></span>
					</div>
				</div>
			</div>
			<div id="preview-container-main"></div>
			<textarea spellcheck="false" id="message" class="uk-textarea field" rows="5" name="message" placeholder="<%= lang.order.form.message%>"
			required></textarea>
			<button class="button" id="batch-upload" type="submit"><%= lang.order.form.call%></button>
		</form>
	</div>
</div>




